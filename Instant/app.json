{
  "expo": {
    "name": "Instant",
    "slug": "Instant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "default",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,

      "bundleIdentifier": "com.anonymous.Instant",
      "infoPlist": "NSFaceIDUsageDescription"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Instant"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "Instant"
  }
}
